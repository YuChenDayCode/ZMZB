.treetable {
	margin-left: 5px;
	border: solid 1px #B7D1EA;
	border-collapse: collapse;
	font-size: 12px;
	color: #023266;
}

.tableth {
border: 0;
	height: 26px;
	border: solid 1px #B7D1EA;
	border-collapse: collapse;
	background: url("../images/table_th.gif");
	font-size: 13px;
	border-collapse: collapse;
}

/* Body
 * ------------------------------------------------------------------------- */
table tbody tr td {
  cursor: default;
  padding: .3em 1.5em;
}

table tbody tr.even {
  background: #f3f3f3;
}

table tbody tr.odd {
  background: #fff;
}

table span {
  background-position: center left;
  background-repeat: no-repeat;
  padding: .2em 0 .2em 1.5em;
}

table span.file {
  background-image: url(../images/page_white_text.png);
}

table span.folder {
  background-image: url(../images/folder.png);
}